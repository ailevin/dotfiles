[advice]
    statusHints = false
[user]
    name = Josh Davis
    email = joshuadavis42@gmail.com
[github]
    user = jdavis
[core]
    editor = /usr/bin/vim
    excludesfile = ~/.gitignore
[help]
    autocorrect = 1
[color]
    ui = true
[alias]
    # Shortcuts
    a = add
    b = branch
    c = commit -v
    ch = checkout
    cl = clone
    d = diff
    dc = diff --cached
    f = fetch
    h = help
    l = log
    r = remote
    rb = rebase
    rf = reflog
    re = reset
    rs = reset
    rv = revert
    s = status
    sl = shortlog
    st = stash
    un = reset HEAD
    unstage = reset HEAD

    # New Commands
    count = shortlog -sn
