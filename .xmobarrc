Config { font = "xft:monaco-8" , bgColor = "#000000" , fgColor = "#ececec"
	, position = Top
	, lowerOnStart = True 
	, commands =
		[ Run Cpu ["-L","3","-H","50","--normal","#429942","--high","#A36666"] 20
		, Run CoreTemp ["-t","Core0: <core0> Core1: <core1>C","-L","40","-H","60","-l","lightblue","-n","gray90","-h","red"] 50
		, Run Memory ["-t","RAM: <usedratio>%"] 100
		, Run Date "%a   %b %_d, %Y  -  %I:%M:%S" "date" 10
		, Run Battery ["-t", "Batt: <left>"] 20
		, Run Weather "KDSM" ["-t","Weather: - <skyCondition>  -  <tempF>F","-L","50","-H","90","--normal","green","--high","red","--low","lightblue"] 36000
		, Run Com "sh" ["~/Scripts/ip.sh"] "ipaddr" 50
		, Run Com "curl" ["-s http://isitgoingtoraintoday.com | grep result | cut -d '>' -f2 | cut -d '<' -f1"] "rain" 36000
		, Run Com "sh" ["~/Scripts/keyboard.sh"] "key" 50
		, Run Com "sh" ["~/Scripts/socks.sh"] "socks" 50
		, Run StdinReader
		]
	, sepChar = "%"
	, alignSep = "}{"
	, template = " %StdinReader% }{<fc=#0b8bff>[</fc>%key%<fc=#0b8bff>]</fc>   -   <fc=#0b8bff>[</fc>%KDSM%  -  Rain: %rain%<fc=#0b8bff>]</fc>   -   <fc=#0b8bff>[</fc>%battery%<fc=#0b8bff>]</fc>   -   <fc=#0b8bff>[</fc>%cpu% -  %coretemp%<fc=#0b8bff>]</fc>   -   <fc=#0b8bff>[</fc>%memory%<fc=#0b8bff>]</fc>   -   <fc=#0b8bff>[</fc>ip: %ipaddr% - %socks%<fc=#0b8bff>]</fc>     --     <fc=#0b8bff>[</fc>%date%<fc=#0b8bff>]</fc>     "
       }
